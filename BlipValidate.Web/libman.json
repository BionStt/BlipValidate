{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "node_modules/bootstrap/dist/css/",
      "destination": "wwwroot/lib/bootstrap/dist/css/",
      "files": [
        "bootstrap.css"
      ]
    },
    {
      "library": "node_modules/jquery/dist/",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [
        "core.js",
        "jquery.js",
        "jquery.min.js"
      ]
    },
    {
      "library": "node_modules/bootstrap/dist/js/",
      "destination": "wwwroot/lib/bootstrap/dist/js/",
      "files": [
        "bootstrap.min.js"
      ]
    },
    {
      "library": "node_modules/jquery-validation/dist/",
      "destination": "wwwroot/lib/jquery-validation/dist/"
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@1.14.6",
      "destination": "wwwroot/lib/popper.js/dist/",
      "files": [
        "umd/popper.js",
        "umd/popper.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "jquery.validate.js",
        "additional-methods.min.js",
        "additional-methods.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "core.js"
      ]
    }
  ]
}